{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "demo-schematics": {
      "description": "A demo schematic.",
      "factory": "./demo-schematics/index#demoSchematics"
    },
    "demo-options": {
      "description": "A schematic with options.",
      "factory": "./demo-options/index#demo"
    },
    "demo-schema": {
      "description": "A schematic with options passed as a 'Schema' class.",
      "factory": "./demo-schema/index#demo",
      "schema": "./demo-schema/schema.json"
    },
    "demo-template": {
      "description": "A schematic that uses templates to create a component.",
      "factory": "./demo-template/index#demo",
      "schema": "./demo-template/schema.json"
    },
    "demo-ng": {
      "description": "A schematic that reproduce the ng generate behavior.",
      "factory": "./demo-ng/index#demo",
      "schema": "./demo-ng/schema.json"
    },
    "demo-modify-module": {
      "description": "A schematic that modifies a module to declare a component.",
      "factory": "./demo-modify-module/index#demo",
      "schema": "./demo-modify-module/schema.json"
    }
  }
}
